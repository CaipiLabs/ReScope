{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
//  "opts": {
//    "template": "libs/jsdoc-one-page"
//  },
  "plugins": ["jsdoc-export-default-interop/dist/index", "plugins/markdown"],
  "templates": {
//    "sort": true,
    "static": true,
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
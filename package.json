{
  "name": "rescope",
  "version": "4.3.9",
  "description": "Flexible state management system based on flux architecture, stores data components & inheritable scopes",
  "repository": "https://github.com/n8tz/rescope.git",
  "main": "./etc/npm/index.js",
  "license": "MIT",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "rootAlias": "RS",
        "externals": true,
        "production": true,
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "staging": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "rootAlias": "RS",
        "externals": true,
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "samples": {
      "rootFolder": "src",
      "basedOn": "ComponentSamples",
      "vars": {
        "rootAlias": "RS",
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi",
    "test": "./node_modules/.bin/mocha --require @babel-core/register ./etc/test/*.test.js -b"
  },
  "dependencies": {
    "is": "^3.2.1",
    "shortid": "^2.2.8",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "babel-preset-react": "^6.24.1",
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.18.11",
    "webpack-inherit": "^1.4.10",
    "wpi-react-hmr-ssr": "^1.1.4"
  }
}

{
  "name": "rescope",
  "version": "4.3.9",
  "description": "Flexible state management system based on flux architecture, stores data components & inheritable scopes",
  "repository": "https://github.com/n8tz/rescope.git",
  "main": "./etc/npm/index.js",
  "license": "MIT",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "rootAlias": "RS",
        "externals": true,
        "production": true,
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "staging": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "rootAlias": "RS",
        "externals": true,
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "samples": {
      "rootFolder": "src",
      "basedOn": "ComponentSamples",
      "vars": {
        "rootAlias": "RS",
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi && wpi :samples",
    "staging": "wpi :staging && wpi :samples",
    "dev": "wpi-dev-server :samples --hot",
    "runBrowser": "start http://localhost:8080"
  },
  "dependencies": {
    "is": "^3.2.1",
    "shortid": "^2.2.8",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "wpi-react-hmr-ssr": "^1.1.4",
    "webpack-inherit": "^1.4.10",
    "mocha": "^3.5.1"
  }
}
